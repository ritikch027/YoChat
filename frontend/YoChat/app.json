{
  "expo": {
    "name": "YoChat",
    "slug": "YoChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashImage.png",
    "scheme": "yochat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/splashImage.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "hello.world.apk"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/splashImage.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "18121ba5-9f02-4940-b689-6ba5b9354fe1"
      }
    },
    "owner": "ritikch027"
  }
}
